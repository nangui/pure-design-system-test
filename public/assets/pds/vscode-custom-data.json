      "attributes": [
        {
          "name": "layout",
          "description": "The layout direction of the panels. Can be \"horizontal\" or \"vertical\". Defaults to \"horizontal\"."
        },
        {
          "name": "defaultsplit",
          "description": "The initial size of the primary (left/top) panel. Defaults to \"450px\"."
        },
        {
          "name": "breakpoint",
          "description": "The viewport width in pixels below which the component switches to mobile view. Defaults to 1024."
        },
        {
          "name": "open",
          "description": "Controls the visibility of the primary panel in mobile view."
        }
      ]
    },
    {
      "name": "pds-tabpanel",
      "description": "A tab panel used as a child of `<pds-tabstrip>`. Each panel becomes a tab\nwith its button label derived from the `label` attribute.",
      "attributes": [
        {
          "name": "label",
          "description": "Label text displayed on the tab button"
        },
        {
          "name": "id",
          "description": "Unique identifier for the panel (auto-generated if not provided)"
        }
      ]
    },
    {
      "name": "pds-tabstrip",
      "description": "Tab navigation component that displays content in switchable panels.\n\nUse `<pds-tabpanel>` children with a `label` attribute to define each tab.\nThe component auto-generates navigation buttons and handles URL hash synchronization.",
      "attributes": [
        {
          "name": "label",
          "description": "Accessible label announced for the tablist (default: \"Tabs\")"
        },
        {
          "name": "selected",
          "description": "Identifier of the currently active panel (synced with the location hash)"
